<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".layoutvendor">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
        <ImageView
            android:id="@+id/ImageViewToko1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:srcCompat="@drawable/ic_search_icon"
            tools:ignore="VectorDrawableCompat"
            android:padding="5dp"
            android:layout_marginRight="260dp"
            android:onClick="searchVendor"/>
        <ImageView
            android:id="@+id/ImageViewToko2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:srcCompat="@drawable/ic_box"
            tools:ignore="VectorDrawableCompat"
            android:padding="5dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@+id/ImageViewToko1"
            android:onClick="boxvendor"/>
        <ImageView
            android:id="@+id/ImageViewToko3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:srcCompat="@drawable/ic_notif"
            tools:ignore="VectorDrawableCompat"
            android:padding="5dp"
            android:layout_marginRight="300dp"
            android:layout_toRightOf="@+id/ImageViewToko2"
            android:onClick="notifvendor"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/TextViewVendor1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pilih vendor berdasarkan"
                android:textStyle="bold"
                android:textColor="@color/Black"
                android:layout_marginBottom="10dp"/>
            <Button
                android:id="@+id/ButtonVendor1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/borderbuttongrey"
                android:text="Semua Kategori"
                android:layout_gravity="start"
                android:textColor="#666666"
                android:textAlignment="textStart"
                android:paddingLeft="20dp"/>
            <Button
                android:id="@+id/ButtonVendor2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/borderbuttongrey"
                android:text="Kota (Medan)"
                android:layout_gravity="start"
                android:textColor="#666666"
                android:textAlignment="textStart"
                android:paddingLeft="20dp"/>
            <Button
                android:id="@+id/ButtonVendor3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/borderbuttongrey"
                android:text="Budget"
                android:layout_gravity="start"
                android:textColor="#666666"
                android:textAlignment="textStart"
                android:paddingLeft="20dp"/>
            <Button
                android:id="@+id/ButtonVendor4"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/borderbuttongrey"
                android:text="Tampilkan hanya yang dapat menjadwalkan ulang"
                android:layout_gravity="start"
                android:textSize="12dp"
                android:textColor="#666666"
                android:textAlignment="textStart"
                android:paddingLeft="20dp"/>
            <Button
                android:id="@+id/ButtonVendor5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/maincolor"
                android:text="Cari Vendor"
                android:textColor="@color/white"
                android:onClick="halamanvendor"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="20dp">
            <TextView
                android:id="@+id/TextViewVendor2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/Black"
                android:textStyle="bold"
                android:text="Cari berdasarkan kategori"
                android:layout_marginBottom="15dp"/>
            <ImageView
                android:id="@+id/ImageViewVendor1"
                android:layout_width="180dp"
                android:layout_height="100dp"
                android:background="@drawable/marryimg4"
                android:layout_below="@id/TextViewVendor2"/>
            <TextView
                android:id="@+id/textveiwvendor3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reservation place"
                android:textColor="@color/Black"
                android:layout_below="@id/ImageViewVendor1"/>
            <ImageView
                android:id="@+id/ImageViewVendor2"
                android:layout_width="180dp"
                android:layout_height="100dp"
                android:background="@drawable/marryimg5"
                android:layout_below="@id/TextViewVendor2"
                android:layout_toRightOf="@id/ImageViewVendor1"
                android:layout_marginLeft="20dp"/>
            <TextView
                android:id="@+id/textveiwvendor4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Decoration"
                android:textColor="@color/Black"
                android:layout_below="@id/ImageViewVendor1"
                android:layout_toRightOf="@id/textveiwvendor3"
                android:layout_marginLeft="90dp"/>
        </RelativeLayout>
    </LinearLayout>
    <View
        android:id="@+id/viewhomedivider"
        android:background="@color/Black"
        android:layout_marginTop="3dp"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginBottom="4dp"/>
    <RelativeLayout
        android:layout_marginTop="3dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/imageviewdivH1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/home"
            android:layout_marginLeft="20dp"
            android:onClick="vendortohome"/>
        <TextView
            android:id="@+id/textviewdivH1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH1"
            android:text="Home"
            android:layout_marginLeft="17dp"/>
        <ImageView
            android:id="@+id/imageviewdivH2"
            android:layout_width="30dp"
            android:layout_height="25dp"
            android:background="@mipmap/vendor"
            android:layout_toRightOf="@id/imageviewdivH1"
            android:layout_marginLeft="70dp"/>
        <TextView
            android:id="@+id/textviewdivH2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH2"
            android:layout_toRightOf="@id/textviewdivH1"
            android:text="Vendor"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="-5dp"/>
        <ImageView
            android:id="@+id/imageviewdivH3"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/toko"
            android:layout_toRightOf="@id/imageviewdivH2"
            android:layout_marginLeft="45dp"
            android:onClick="vendortotoko"/>
        <TextView
            android:id="@+id/textviewdivH3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH3"
            android:layout_toRightOf="@id/textviewdivH2"
            android:text="Toko"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="-7dp"/>
        <ImageView
            android:id="@+id/imageviewdivH4"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/ide"
            android:layout_toRightOf="@id/imageviewdivH3"
            android:layout_marginLeft="40dp"
            android:onClick="vendortoide"/>
        <TextView
            android:id="@+id/textviewdivH4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH4"
            android:layout_toRightOf="@id/textviewdivH3"
            android:text="Ide"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="-7dp"/>
        <ImageView
            android:id="@+id/imageviewdivH5"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/profil"
            android:layout_toRightOf="@id/imageviewdivH4"
            android:layout_marginLeft="60dp"
            android:onClick="vendortoprofile"/>
        <TextView
            android:id="@+id/textviewdivH5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH5"
            android:layout_toRightOf="@id/textviewdivH4"
            android:text="Ide"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="-7dp"/>
    </RelativeLayout>
</LinearLayout>