    <View
        android:id="@+id/viewhomedivider"
        android:background="@color/Black"
        android:layout_marginTop="23dp"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginBottom="4dp"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/imageviewdivH1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/home"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/textviewdivH1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH1"
            android:text="Home"
            android:layout_marginLeft="17dp"/>
        <ImageView
            android:id="@+id/imageviewdivH2"
            android:layout_width="30dp"
            android:layout_height="25dp"
            app:srcCompat="@mipmap/vendor"
            android:layout_toRightOf="@id/imageviewdivH1"
            android:layout_marginLeft="70dp"
            android:onClick="hometovendor"/>
        <TextView
            android:id="@+id/textviewdivH2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH2"
            android:layout_toRightOf="@id/textviewdivH1"
            android:text="Vendor"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="-5dp"/>
        <ImageView
            android:id="@+id/imageviewdivH3"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/toko"
            android:layout_toRightOf="@id/imageviewdivH2"
            android:layout_marginLeft="45dp"
            android:onClick="hometotoko"/>
        <TextView
            android:id="@+id/textviewdivH3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH3"
            android:layout_toRightOf="@id/textviewdivH2"
            android:text="Toko"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="-7dp"/>
        <ImageView
            android:id="@+id/imageviewdivH4"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/ide"
            android:layout_toRightOf="@id/imageviewdivH3"
            android:layout_marginLeft="40dp"
            android:onClick="hometoide"/>
        <TextView
            android:id="@+id/textviewdivH4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH4"
            android:layout_toRightOf="@id/textviewdivH3"
            android:text="Ide"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="-7dp"/>
        <ImageView
            android:id="@+id/imageviewdivH5"
            android:layout_width="35dp"
            android:layout_height="30dp"
            android:layout_marginTop="-3dp"
            android:background="@mipmap/profil"
            android:layout_toRightOf="@id/imageviewdivH4"
            android:layout_marginLeft="60dp"
            android:onClick="hometoprofil"/>
        <TextView
            android:id="@+id/textviewdivH5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageviewdivH5"
            android:layout_toRightOf="@id/textviewdivH4"
            android:text="Ide"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="-7dp"/>
    </RelativeLayout>
</LinearLayout>