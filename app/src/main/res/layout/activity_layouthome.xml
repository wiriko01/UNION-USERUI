                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0012"
                    android:layout_toRightOf="@id/ImageViewHome25"/>
                <ImageView
                    android:id="@+id/ImageViewHome27"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0012"
                    android:layout_toRightOf="@id/ImageViewHome26"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp">
                <ImageView
                    android:id="@+id/ImageViewHome28"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:background="@drawable/weddingplanning2"/>
                <TextView
                    android:id="@+id/TextViewHome013"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Wedding Planning 2"
                    android:textColor="@color/Black"
                    android:layout_below="@id/ImageViewHome28"/>
                <ImageView
                    android:id="@+id/ImageViewHome29"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome013"/>
                <ImageView
                    android:id="@+id/ImageViewHome30"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome013"
                    android:layout_toRightOf="@id/ImageViewHome29"/>
                <ImageView
                    android:id="@+id/ImageViewHome31"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome013"
                    android:layout_toRightOf="@id/ImageViewHome30"/>
                <ImageView
                    android:id="@+id/ImageViewHome32"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome013"
                    android:layout_toRightOf="@id/ImageViewHome31"/>
                <ImageView
                    android:id="@+id/ImageViewHom33"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome013"
                    android:layout_toRightOf="@id/ImageViewHome32"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp">
                <ImageView
                    android:id="@+id/ImageViewHome34"
                    android:layout_width="150dp"
                    android:layout_height="100dp"
                    android:background="@drawable/weddingplanning3"/>
                <TextView
                    android:id="@+id/TextViewHome0014"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Wedding Planning 2"
                    android:textColor="@color/Black"
                    android:layout_below="@id/ImageViewHome34"/>
                <ImageView
                    android:id="@+id/ImageViewHome35"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0014"/>
                <ImageView
                    android:id="@+id/ImageViewHome36"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0014"
                    android:layout_toRightOf="@id/ImageViewHome35"/>
                <ImageView
                    android:id="@+id/ImageViewHome37"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0014"
