                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0014"
                    android:layout_toRightOf="@id/ImageViewHome37"/>
                <ImageView
                    android:id="@+id/ImageViewHome39"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@mipmap/ic_star"
                    android:layout_below="@id/TextViewHome0014"
                    android:layout_toRightOf="@id/ImageViewHome38"/>
            </RelativeLayout>
        </LinearLayout>
